{
  "kind": "collectionType",
  "collectionName": "educational_programs",
  "info": {
    "singularName": "educational-program",
    "pluralName": "educational-programs",
    "displayName": "Educational program",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "maxLength": 255,
      "required": true
    },
    "code": {
      "type": "string",
      "maxLength": 255
    },
    "mainName": {
      "type": "string",
      "maxLength": 255
    },
    "mainCode": {
      "type": "string",
      "maxLength": 255
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "bachelor",
        "magistracy",
        "postgraduate"
      ],
      "required": true
    },
    "order": {
      "type": "integer",
      "unique": true
    },
    "graduates": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::graduate.graduate",
      "mappedBy": "educational_programs"
    }
  }
}
