{
  "kind": "collectionType",
  "collectionName": "educational_programs",
  "info": {
    "singularName": "educational-program",
    "pluralName": "educational-programs",
    "displayName": "~E: Образовательные программы",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "maxLength": 255,
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "code": {
      "type": "string",
      "maxLength": 255
    },
    "mainName": {
      "type": "string",
      "maxLength": 255,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "mainCode": {
      "type": "string",
      "maxLength": 255
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "maxLength": 255,
      "required": true,
      "unique": false,
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "bachelor",
        "magistracy",
        "postgraduate"
      ],
      "required": true
    },
    "order": {
      "type": "integer",
      "unique": false
    },
    "graduates": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::graduate.graduate",
      "mappedBy": "educational_programs"
    },
    "department": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::department.department",
      "inversedBy": "educationalPrograms"
    },
    "employees": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::employee.employee",
      "inversedBy": "educationalPrograms"
    },
    "content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "content.accordion",
        "content.bento-grid",
        "content.collection-all",
        "content.contacts",
        "content.files-grid",
        "content.files",
        "content.form-block",
        "content.icons-block",
        "content.numbers",
        "content.slider-entity",
        "content.slider-photos",
        "content.slider-video",
        "content.text-block",
        "content.text-grid",
        "content.text-images",
        "content.text-video",
        "content.timeline"
      ],
      "required": true
    }
  }
}
