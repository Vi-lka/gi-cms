{
  "kind": "collectionType",
  "collectionName": "dpo_courses",
  "info": {
    "singularName": "dpo-course",
    "pluralName": "dpo-courses",
    "displayName": "~E: Курсы ДПО",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "dateStart": {
      "type": "date"
    },
    "dateEnd": {
      "type": "date"
    },
    "hours": {
      "type": "integer"
    },
    "price": {
      "type": "string",
      "maxLength": 255
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "order": {
      "type": "integer",
      "unique": false
    },
    "location": {
      "type": "string",
      "maxLength": 255,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "text",
      "maxLength": 300
    },
    "department": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::department.department",
      "inversedBy": "dpoCourses"
    },
    "employees": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::employee.employee",
      "inversedBy": "dpoCourses"
    },
    "graduates": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::graduate.graduate",
      "inversedBy": "dpoCourses"
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "maxLength": 255,
      "required": true,
      "unique": false,
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    },
    "content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "content.accordion",
        "content.bento-grid",
        "content.collection-all",
        "content.contacts",
        "content.files-grid",
        "content.files",
        "content.form-block",
        "content.icons-block",
        "content.numbers",
        "content.slider-entity",
        "content.slider-photos",
        "content.text-block",
        "content.text-grid",
        "content.text-images",
        "content.timeline"
      ],
      "required": true
    }
  }
}
