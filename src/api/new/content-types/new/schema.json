{
  "kind": "collectionType",
  "collectionName": "news",
  "info": {
    "singularName": "new",
    "pluralName": "news",
    "displayName": "~I: Новости",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "text": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "blocks",
      "required": false
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$",
      "required": true,
      "unique": false,
      "maxLength": 255
    },
    "content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "content.accordion",
        "content.bento-grid",
        "content.collection-all",
        "content.contacts",
        "content.files-grid",
        "content.files",
        "content.form-block",
        "content.icons-block",
        "content.numbers",
        "content.slider-entity",
        "content.slider-photos",
        "content.slider-video",
        "content.text-block",
        "content.text-grid",
        "content.text-images",
        "content.text-video",
        "content.timeline"
      ],
      "required": true
    }
  }
}
