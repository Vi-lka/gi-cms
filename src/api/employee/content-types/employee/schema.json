{
  "kind": "collectionType",
  "collectionName": "employees",
  "info": {
    "singularName": "employee",
    "pluralName": "employees",
    "displayName": "~P: Сотрудники",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "slug": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "maxLength": 255,
      "required": true,
      "unique": false,
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
    },
    "description": {
      "type": "text",
      "maxLength": 500,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string",
      "maxLength": 255
    },
    "location": {
      "type": "string",
      "maxLength": 255,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "hashtags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::hashtag.hashtag",
      "inversedBy": "employees"
    },
    "meta": {
      "displayName": "meta",
      "type": "component",
      "repeatable": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "employee.meta",
      "required": false
    },
    "order": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "biginteger"
    },
    "showContacts": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": false,
      "required": true
    },
    "showHashtags": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "boolean",
      "default": true,
      "required": true
    },
    "head_in_department": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::department.department",
      "mappedBy": "head"
    },
    "departments": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::department.department",
      "mappedBy": "employees"
    },
    "educationalPrograms": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::educational-program.educational-program",
      "mappedBy": "employees"
    },
    "dpoCourses": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::dpo-course.dpo-course",
      "mappedBy": "employees"
    },
    "content": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "dynamiczone",
      "components": [
        "content.accordion",
        "content.bento-grid",
        "content.collection-all",
        "content.contacts",
        "content.files-grid",
        "content.files",
        "content.form-block",
        "content.icons-block",
        "content.numbers",
        "content.slider-entity",
        "content.slider-photos",
        "content.slider-video",
        "content.text-block",
        "content.text-grid",
        "content.text-images",
        "content.text-video",
        "content.timeline"
      ],
      "required": true
    },
    "events": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::event.event",
      "mappedBy": "employees"
    },
    "eduEducationalPrograms": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::edu-educational-program.edu-educational-program",
      "mappedBy": "employees"
    }
  }
}
